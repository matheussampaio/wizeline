{
    "extends": "airbnb-base",
    "rules": {
        "func-names": 0,
        "no-param-reassign": 0,
        "padded-blocks": 0,
        "comma-dangle": [2, "never"],
        "spaced-comment": 0,
        "arrow-body-style": 0,
        "indent": ["error", 4],
        "max-len": ["error", 120],
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
        "class-methods-use-this": 0,
        "no-underscore-dangle": 0
    }
}
